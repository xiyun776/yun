 #include<iostream>
#include<stdio.h>
using namespace std;
#define ll unsigned long long
ll a, b, c, re = 1;;
int main()
{
    cin >> a >> b >> c; ll n = a, m = b;
    while (b >= 1)
    {
        if (b & 1)re = re * a % c;
        b = b >> 1;
        a = a * a % c;
    }
    cout << n << "^" << m << " mod "<<c<<"="<< re;
    return 0;
}
当然了
